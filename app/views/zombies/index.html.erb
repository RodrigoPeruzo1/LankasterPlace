    <div id="conteudo">
  <h3>Zombie Control </h3>    
<p id="notice"><%= notice %></p>
 
<marquee><h2>Zombies da sala 309</h2></marquee>

<center><h1>Zombies</h1>
<div class="div">
<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Bio</th>
      <th>Age</th>
      <th>Roting</th>
      <th>Salary</th>
      <th>Height</th>
      <th>Premium</th>
      <th>Zombification</th>
      <th>Lunch</th>
      <th>Born</th>
      <th colspan="3"></th>
    </tr>
  </thead>
</div>
  <tbody>
    <% @zombies.each do |zombie| %>
      <tr>
        <td><%= zombie.name %></td>
        <td><%= zombie.bio %></td>
        <td><%= zombie.age %></td>
        <td><%= zombie.roting %></td>
        <td><%= zombie.salary %></td>
        <td><%= zombie.height %></td>
        <td><%= zombie.premium %></td>
        <td><%= zombie.zombification %></td>
        <td><%= zombie.lunch %></td>
        <td><%= zombie.born %></td>
        <td><%= link_to 'Show', zombie %></td>
        <td><%= link_to 'Edit', edit_zombie_path(zombie) %></td>
        <td><%= link_to 'Destroy', zombie, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>


</center>
